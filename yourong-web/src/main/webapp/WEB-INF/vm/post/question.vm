#set($title="帮助中心-新手指导-有融网-全透明的互联网理财优选平台")#set($keywords="帮助中心,新手指导,有融网")#set($description="有融网运用高效的运营管理体系和创新的互联网思维,搭建了一个安全透明的互联网理财服务平台. ")##新手指引#parse('/common/common.vm')#set($css_url=["$root_css_url/post/help.css","$root_css_url/modules/default.css"])#set($js_url=[    "/static/lib/js/vue/vue.js",    "$root_js_url/post/helpCenter.js",    "$root_js_url/activity/activityCommon/activityNav.js"])#CSRFToken('xToken')<div id="helpContent" v-cloak>    <div id="u-top-banner" class="u-top-banner">        <p class="u-title">我们能为你提供什么帮助吗？</p>        <ul class="u-tab">            <li v-for="(index,item) in tabs" :class="{'z-selected':tabIndex===index}" @click="changeTab(index)">                {{item.title}}            </li>        </ul>    </div>    <div class="u-guide-wrap">    ##    热门问题        <div class="u-guide-content" v-show="tabIndex === 0">            <p class="title" v-if="tabIndex == 0">热门问题</p>            <ul class="u-guide-items">                <li class="item-detail" v-for="(index,item) in init" @click="count(item.id,$event)" id="1">                    <h2>{{{item.content}}}<i>+</i></h2>                    <div class="u-demand f-dn">                        {{{item.answer}}}                    </div>                </li>            </ul>        </div>    ##    常见问题        <div class="u-guide-content" v-show="tabIndex === 1">            <p class="title common-title" v-if="tabIndex == 1">常见问题</p>            <ul class="question-category">                <li v-for="(index,item) in q_category"                    :class="{'z-selected':cateIndex===index}"                    @click="changeCate(index)"                >                    {{item.value}}                </li>            </ul>            <div class="u-guide-items">                <div class="cate" v-show="cateIndex === 0">                    <div class="topic" v-for="(key,val) in aAccount">                        <div class="topic-tag">                            <p>{{$key}}</p>                        </div>                        <div class="topic-content" v-for="item in val">                            <h2 @click="count(item.id,$event)">{{{item.content}}}<i>+</i></h2>                            <div class="u-demand f-dn">                                {{{item.answer}}}                            </div>                        </div>                    </div>                </div>                <div class="cate" v-show="cateIndex === 1">                    <div class="topic" v-for="(key,val) in aFund">                        <div class="topic-tag">                            <p>{{$key}}</p>                        </div>                        <div class="topic-content" v-for="item in val">                            <h2 @click="count(item.id,$event)">{{{item.content}}}<i>+</i></h2>                            <div class="u-demand f-dn">                                {{{item.answer}}}                            </div>                        </div>                    </div>                </div>                <div class="cate" v-show="cateIndex === 2">                    <div class="topic" v-for="(key,val) in aInvest">                        <div class="topic-tag">                            <p>{{$key}}</p>                        </div>                        <div class="topic-content" v-for="item in val">                            <h2 @click="count(item.id,$event)">{{{item.content}}}<i>+</i></h2>                            <div class="u-demand f-dn">                                {{{item.answer}}}                            </div>                        </div>                    </div>                </div>                <div class="cate" v-show="cateIndex === 3">                    <div class="topic" v-for="(key,val) in aFn">                        <div class="topic-tag">                            <p>{{$key}}</p>                        </div>                        <div class="topic-content" v-for="item in val">                            <h2 @click="count(item.id,$event)">{{{item.content}}}<i>+</i></h2>                            <div class="u-demand f-dn">                                {{{item.answer}}}                            </div>                        </div>                    </div>                </div>                <div class="cate" v-show="cateIndex === 4">                    <div class="topic" v-for="(key,val) in aActivity">                        <div class="topic-tag">                            <p>{{$key}}</p>                        </div>                        <div class="topic-content" v-for="item in val">                            <h2 @click="count(item.id,$event)">{{{item.content}}}<i>+</i></h2>                            <div class="u-demand f-dn">                                {{{item.answer}}}                            </div>                        </div>                    </div>                </div>            </div>        </div>    ##        新手引导        <div class="u-guide-content" v-show="tabIndex === 2">            <p class="title" v-if="tabIndex == 2">新手引导</p>            <ul class="u-guide-items">                <li class="item-detail" v-for="(index,item) in init" @click="count(item.id,$event)">                    <h2>Q{{index+1}}. {{{item.content}}}<i>+</i></h2>                    <div class="u-demand f-dn">                        {{{item.answer}}}                    </div>                </li>            </ul>        </div>    </div></div>####		右侧悬浮二维码<div class="u-floating-window" id="j-floating-window">    <span class="u-icon-robot"></span>    <a href="http://wpa.b.qq.com/cgi/wpa.php?ln=2&amp;uin=4000266688" target="_blank" class="u-icon-kefu f-pos-r"       id="j-icon-kefu">    </a>    <a class="u-icon-weibo f-pos-r" href="http://weibo.com/5248978316/" target="_blank" id="j-icon-weibo">			<span class="f-dn">				<i class="f-fs20"></i>				<img src="/static/img/common/new-weibo-qrcode.png">			</span>    </a>    <a class="u-icon-weixin f-pos-r" id="j-icon-app">			<span class="f-dn">				<i class="f-fs20"></i>				<img src="/static/img/common/new-weixin-qrcode.png">			</span>    </a>    <a class="u-icon-app f-pos-r" id="j-icon-weixin">			<span class="f-dn">				<i class="f-fs20"></i>				<img src="/static/img/common/new-app-qrcode.png">			</span>    </a>    <a class="u-icon-top f-pos-r" id="j-icon-gotop"></a></div>