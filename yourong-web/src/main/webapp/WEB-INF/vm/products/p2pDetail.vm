    <script>

        var vm = new Vue({
            el: '#j-direct',
            data: {
                open: false,
				pInfoStatus: $!{projectInfo.status},                           //项目状态
                onlineCountDown: $!{projectInfo.onlineCountDown},              //快投预告倒计时
                lotteryEndCountDown: $!{projectInfo.lotteryEndCountDown}       //快投抽奖结束时间
            },
            created: function () {
                var self = this
                if (self.onlineCountDown && self.onlineCountDown>0) {
                    self.lotteryEndCountDown = self.lotteryEndCountDown - self.onlineCountDown
                }

                $("#j-agreement").click(function () {
                    $('.j-credit-prove').addClass('z-current').siblings().removeClass('z-current');
                });
            },
            methods: {
                switchTip: function (index) {
                    this.open = index
                },
                refresh: function () {
                    window.location.reload()
                },
                noticeEndEvent: function () {
					this.pInfoStatus = 30
					this.onlineCountDown = 0
                },
                lotteryEndEvent: function () {
					this.lotteryEndCountDown = 0
                }
            }
        });
        var directProjectInfo = {
            "riskInfo":$!{projectInfo.description},
            "collateral":$!{collateral.collateralDetails}
        }
    </script>
