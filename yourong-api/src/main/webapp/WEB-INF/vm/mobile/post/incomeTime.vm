##我的存钱罐页面收益时间表
#parse('/mobile/common.vm')
#set($title='收益时间表')

<style>
    h1 {
        color: #666666;
        font-weight: 100;
        font-size: 14px;
        margin: 20px 0 12px 10px;
    }

    table {
        margin: 0 auto;
        border-left: 1px solid #ccc;
        width: 95%;
        border-spacing: 0;
    }

    table thead {
        background: #E5E5E5;
    }

    table thead tr {
        background: #E5E5E5;
    }

    table th {
        text-align: center;
        height: 2.5rem;
        font-weight: 100;
        color: #333333;
        font-size: 12px;
        border-top: 1px solid #ccc;
        border-right: 1px solid #ccc;
        width: 30%;
    }

    table td {
        text-align: center;
        height: 3.75rem;
        font-weight: 100;
        color: #333333;
        font-size: 12px;
        border-right: 1px solid #ccc;
        border-bottom: 1px solid #ccc;
        width: 30%;
        margin: 0;
        padding: 0;
    }

    table td i {
        color: #999;
        font-size: 12px;
        font-style: normal;
        font-weight: 100;
    }

    @media screen and (min-width: 390px) {
        html {
            font-size: 22px;
        }
    }

    @media screen and (min-width: 360px) and (max-width: 389px) {

        html {
            font-size: 18px;
        }
    }

    @media screen and (max-width: 359px) {

    }


</style>
