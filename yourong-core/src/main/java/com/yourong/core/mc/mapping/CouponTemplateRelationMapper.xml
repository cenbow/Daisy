<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yourong.core.mc.dao.CouponTemplateRelationMapper" >

    <select id="selectByCode" parameterType="java.lang.String" resultType="string">
