<div tabindex="-1" class="modal fade" id="modal-creditGradeInfo" style="display: none;" aria-hidden="true">
    <div class="modal-dialog w700">
        <div class="modal-content">
            <div class="modal-header no-padding">
                <div class="table-header header-color-dark">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">
                        <span class="white">×</span>
                    </button>
                    <span class="modalFormTitle">百度大数据黑名单查询</span>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12 ">
                        <div class="table-responsive ">
                            <table id="coupon-table" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
                                <tr>
                                    <td>查询用户</td>
                                    <td id="borrowerTrueName"></td>
                                </tr>
                                <tr>
                                    <td>手机号</td>
                                    <td id="borrowerMobile"></td>
                                </tr>
                                <tr>
                                    <td>身份证</td>
                                    <td id="identityNumber"></td>
                                </tr>
                                <tr>
                                    <td>查询结果</td>
                                    <td id="blackLevel"></td>
                                </tr>
                                <tr>
                                    <td>黑名单原因</td>
                                    <td id="blackReason"></td>
                                </tr>
                                <tr>
                                    <td>查询时间</td>
                                    <td id="blackQueryTime"></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="close_creditGradeInfo" data-page="1" class="btn btn-primary">确定</button>
            </div>
        </div>
    </div>
</div>

