<div tabindex="-1" class="modal fade" id="closeBonus-table" style="display: none;" aria-hidden="true">
  <div class="modal-dialog w500">
    <div class="modal-content">
      <div class="modal-header no-padding">
        <div class="table-header">
          <button aria-hidden="true" data-dismiss="modal" class="close" type="button">
            <span class="white">×</span>
          </button>
          结算分红
        </div>
      </div>
      <div class="modal-body">
        <form role="form" class="form-horizontal" id="closeBonus_form" method="post">
          <input type="hidden" name="id" id="closebonus_id"/>
          <!--<input type = "hidden" name = "projectId" id = "project_id" >-->
          <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">租赁总收益:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="totalIncome" class="col-xs-10 col-sm-7" placeholder="" id="form-field-totalIncome" readonly></div>
            </div>
          </div>
          <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">用户共分红:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="userBonus" class="col-xs-10 col-sm-6"datatype="/^(?!0+(?:\.0+)?$)(?:[1-9]\d*|0)(?:\.\d{1,2})?$/"  placeholder="" id="form-field-userBonus">
                <span class='input-text'>%</span>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">借款人分红:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="borrowerBonus" class="col-xs-10 col-sm-6" datatype="/^(?!0+(?:\.0+)?$)(?:[1-9]\d*|0)(?:\.\d{1,2})?$/" placeholder="" id="form-field-borrowerBonus">
                <span class='input-text'>%</span>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">出借人分红:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="lenderBonus" class="col-xs-10 col-sm-6" datatype="/^(?!0+(?:\.0+)?$)(?:[1-9]\d*|0)(?:\.\d{1,2})?$/" placeholder="" id="form-field-lenderBonus">
                <span class='input-text'>%</span>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">租赁公司分红:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="lessorBonus" class="col-xs-10 col-sm-6" datatype="/^(?!0+(?:\.0+)?$)(?:[1-9]\d*|0)(?:\.\d{1,2})?$/" placeholder="" id="form-field-lessorBonus">
                <span class='input-text'>%</span>
              </div>
            </div>
          </div>
         <!-- <div class="form-group">
            <label for="form-field-1" class="col-sm-4 control-label no-padding-right">备注:</label>
            <div class="col-sm-8">
              <div class="clearfix">
                <input type="text" name="remarks" class="col-xs-10 col-sm-7" placeholder="" id="form-field-remarks"></div>
            </div>
          </div>-->
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
        <button type="button" id="save_closeBonus" class="btn btn-primary">保存</button>
      </div>
    </div>
    <!-- /.modal-content --> </div>
  <!-- /.modal-dialog -->
</div>