<div tabindex="-1" class="modal fade" id="modal-table" style="display: none;" aria-hidden="true">
     <div class="modal-dialog">
     <div class="modal-content">
 <div class="modal-header no-padding">
             <div class="table-header">
                  <button aria-hidden="true" data-dismiss="modal" class="close" type="button">
                    <span class="white">×</span>
              </button>
    <span class="modalFormTitle">添加数据字典</span>
       </div>
  </div>
   <div class="modal-body">
    <form role="form" class="form-horizontal" id="sysDict_form" method="post">
     <input type="hidden" name="id" />
     
     <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 组名: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="groupName" class="col-xs-10 col-sm-5" placeholder="groupName" id="form-field-group">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 标签名: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="label" class="col-xs-10 col-sm-5" placeholder="label" id="form-field-label">
    </div>
  </div>
</div>
 
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> key值: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="key" class="col-xs-10 col-sm-5" placeholder="key" id="form-field-key">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 数据值: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="value" class="col-xs-10 col-sm-5" placeholder="value" id="form-field-value">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 描述: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="description" class="col-xs-10 col-sm-5" placeholder="description" id="form-field-description">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 排序: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="sort" class="col-xs-10 col-sm-5" placeholder="sort" id="form-field-sort">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 备注: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" name="remarks" class="col-xs-10 col-sm-5" placeholder="remarks" id="form-field-remarks">
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 状态: </label>
  <div class="col-xs-3">
  	<div class="clearfix">
		<input class="ace ace-switch" type="checkbox" id="form-field-status" value="0"/>
		<span class="lbl"></span>
		<input type="hidden" name="status" class="switchInput" value="0"/>
	</div>
	</div>
</div>
 
</form>
</div>
<div class="modal-footer">
<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
<button type="button" id="save_sysDict" class="btn btn-primary">保存</button>
</div>
</div>
<!-- /.modal-content -->
 </div>
<!-- /.modal-dialog -->
</div>
