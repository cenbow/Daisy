<div tabindex="-1" class="modal fade" id="modal-table" style="display: none;" aria-hidden="true">
     <div class="modal-dialog">
     <div class="modal-content">
 <div class="modal-header no-padding">
             <div class="table-header">
                  <button aria-hidden="true" data-dismiss="modal" class="close" type="button">
                    <span class="white">×</span>
              </button>
    <span class="modalFormTitle"></span>
       </div>
  </div>
   <div class="modal-body">
    <form role="form" class="form-horizontal" id="sysDict_form" method="post">
     <input type="hidden" name="id" />
     
     <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 银行简称: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="hidden" id="id" name="bankInfo" value=""/>
     <input type="text" id="simpleName" name="bankInfo" class="col-xs-10 col-sm-5" placeholder="银行简称" id="form-field-group" />
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 银行全称: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" id="fullName" name="bankInfo" class="col-xs-10 col-sm-5" placeholder="银行全称" id="form-field-label" />
    </div>
  </div>
</div>
 
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 银行编码: </label>
  <div class="col-sm-9">
    <div class="clearfix">
     <input type="text" id="code" name="bankInfo" class="col-xs-10 col-sm-5" placeholder="银行编码" id="form-field-key" />
    </div>
  </div>
</div>
 <div class="form-group">
   <label for="form-field-1" class="col-sm-3 control-label no-padding-right"> 状态: </label>
  <div class="col-xs-3">
  	<div class="clearfix">
  		<select id="status" name="bankInfo">
  			<option value="1">使用</option>
  			<option value="-1">不使用</option>
  		</select>
	</div>
	</div>
</div>
 
</form>
</div>
<div class="modal-footer">
<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
<button type="button" id="save_bscBank" class="btn btn-primary">保存</button>
</div>
</div>
